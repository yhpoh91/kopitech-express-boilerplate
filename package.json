{
  "name": "kopitech-boilerplate",
  "version": "1.0.0",
  "description": "Kopitech Express Boilerplate",
  "main": "index.js",
  "entry": [
    "regenerator-runtime/runtime",
    "index.js"
  ],
  "scripts": {
    "start": "NODE_ENV=production node build",
    "dev": "NODE_ENV=development nodemon --exec babel-node src",
    "clean": "rm -rf build",
    "build": "npm run clean && babel src -s -D -d build",
    "test": "mocha \"./{,!(node_modules)/**/}*.test.js\" --require babel-polyfill --require esm"
  },
  "author": "Yee Hui Poh <yhpoh91@gmail.com>",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "helmet": "^3.15.1",
    "http-status": "^1.3.1",
    "joi": "^14.3.1",
    "mongoose": "^5.7.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "esm": "^3.2.25",
    "jest": "^24.1.0",
    "mocha": "^6.0.1",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "^0.13.2"
  }
}
